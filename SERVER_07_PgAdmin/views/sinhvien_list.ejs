<!DOCTYPE html>
<html>
<head>
    <base href="http://localhost:3000/" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>List Sinh viên</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="main.css" />
    <script src="main.js"></script>
</head>
<body>
    <h2>PgAdmin</h2>
    <table border="1" width="500">
        <tr>
            <th>Id</th>
            <th>Ten</th>
            <th>Email</th>
            <th><a href="./sinhvien/them">Thêm</a></th>
        </tr>
        <%data.forEach(function(value){ %>
            <tr>
                    <td><%-value.id%></td>
                    <td><%-value.hoten%></td>
                    <td><%-value.email%></td>
                    <td>
                            <a href="./sinhvien/sua/<%-value.id%>">Sửa</a> - 
                            <a onClick=" return confirm('Bạn có chắc chắn!!!!!')" href="./sinhvien/xoa/<%-value.id%>">Xóa</a>
                    </td>
             </tr>
        <%})%>
       
    </table>
</body>
</html>